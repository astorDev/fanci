{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Animation",
            "request": "launch",
            "type": "dart",
            "program": "flutter/lib/animation/playground.dart"
        },
        {
            "name" : "Flutter Lifecycle",
            "request" : "launch",
            "type" : "dart",
            "program" : "flutter/lib/lifecycle/playground.dart"
        },
        {
            "name" : "Flutter Tabs",
            "request" : "launch",
            "type" : "dart",
            "program" : "flutter/lib/tabs/playground.dart"
        },
        {
            "name" : "Dropdown Playground",
            "request" : "launch",
            "type" : "dart",
            "program" : "flutter/lib/dropdown/playground.dart"
        },
        {
            "name" : "Dropmenu",
            "request" : "launch",
            "type" : "dart", 
            "program" : "flutter/lib/dropmenu/playground.dart"
        },
        {
            "name" : "Overlay Playground",
            "request" : "launch",
            "type" : "dart",
            "program" : "flutter/lib/overlay/playground.dart"
        },
                {
                    "name": ".NET MAUI MacCatalyst Launch",
                    "type": "coreclr",
                    "request": "launch",
                    "preLaunchTask": "build",
                    "program": "${workspaceFolder}/dotnet/maui-blazor/bin/Debug/net10.0-maccatalyst/maccatalyst-arm64/Fanci.Playground.MauiBlazor.app/Contents/MacOS/Fanci.Playground.MauiBlazor",
                    "args": [],
                    "cwd": "${workspaceFolder}",
                    "stopAtEntry": true,
                    "console": "internalConsole"
                },
                {
                    "name": "Attach to .NET MAUI MacCatalyst",
                    "type": "coreclr",
                    "request": "attach",
                    "processId": "${command:pickProcess}",
                    "justMyCode": false
                }
    ]
}